var container = angular.module('container', []);

container.controller('containerController',['$scope','Container', function($scope, Container) {

	$scope.start = function() {
		Container.start({
			id: $scope.container.id;
			HostConfig: $scope.container.HostConfig
		}, function (d) {
			// docker started
		}, function (e) {
			// docker fail to start container
                });
	}
}]);
